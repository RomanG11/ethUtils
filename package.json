{
  "name": "node-typescript-starter",
  "version": "0.0.1",
  "description": "A Typescript starter project for Jio.",
  "author": {
    "name": "Roman"
  },
  "license": "MIT",
  "repository": {
    "url": "URL_HERE",
    "type": "git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "config": {
    "BUILD_TIME": "date +%Y-%m-%dT%H:%M:%S%z"
  },
  "scripts": {
    "prestart": "run-s test",
    "build": "tsc",
    "test": "run-s test:lint test:unit",
    "test:lint": "eslint ./src --ext .ts,.tsx",
    "test:unit": "cross-env CI=1 jest",
    "test:unit:output:coverage": "run-s test:unit -- --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "doc:html": "typedoc",
    "changelog": "changelog",
    "code:prettier": "prettier --write \"./src/**/*.{js,ts,tsx,json,scss}\"",
    "prepare": "npm run test && npm run build && npm run changelog"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:lint && npm run test:unit"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "generate-changelog": "^1.8.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-junit-reporter": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.5"
  }
}
